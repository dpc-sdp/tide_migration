{
    "name": "dpc-sdp/tide_migration",
    "description": "Central place to store all migrations related to Tide Drupal 8 distribution",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "dpc-sdp/tide_core": "^3.1.0",
        "drupal/migrate_plus": "^5.2.0",
        "drupal/migrate_tools": "^5.1.0",
        "drupal/migrate_file": "^2.0.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9.2.9"
    },
    "suggest": {
        "dpc-sdp/tide_api:^3.0.0": "Allows to use Drupal in headless mode"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^10"
            }
        }
    }
}
